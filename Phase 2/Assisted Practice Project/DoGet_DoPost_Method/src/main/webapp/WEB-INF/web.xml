<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>DoGet_DoPost_Method</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>

  </welcome-file-list>
  <servlet>
    <servlet-name>DoGetServlet</servlet-name>
    <servlet-class>DoGetServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DoPostMethod</servlet-name>
    <servlet-class>DoPostMethod</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DoGetServlet</servlet-name>
    <url-pattern>/gethandler</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DoPostMethod</servlet-name>
    <url-pattern>/posthandler</url-pattern>
  </servlet-mapping>
</web-app>